{
  "name": "boilerplate-vue-single-spa",
  "version": "0.0.0",
  "scripts": {
    "start:web:local": "vite --config vite.config.ts --port 3002",
    "start:mfe:local": "npm run build:microfrontend:local && cd dist-mfe && npx http-server --cors -p 3001",
    "build:microfrontend:local": "vue-tsc --noEmit && vite build --config vite.mfe.config.js --mode mfelocal",
    "build:microfrontend:watch:local": "vue-tsc --noEmit && vite build --watch --config vite.mfe.config.js --mode mfelocal",
    "build:local": "vue-tsc --noEmit && vite build",
    "test:unit": "vitest run --environment jsdom",
    "test:unit:watch": "vitest --environment jsdom",
    "test:e2e": "npm run build:local && start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "npm run build:local && start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e --config video=false'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:write",
    "test": "npm run build:local && npm run test:unit && npm run test:e2e:ci",
    "precommit": "lint-staged && npm run lint:fix && npm run test",
    "eslint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "eslint:fix": "npm run eslint:check -- --fix",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,vue,jsx,cjs,mjs,tsx,cts,mts,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,vue,jsx,cjs,mjs,tsx,cts,mts,*rc}",
    "prepush": "npm run test",
    "preview": "vite preview --port 4173"
  },
  "dependencies": {
    "pinia": "^2.0.22",
    "single-spa-vue": "^2.5.1",
    "vite-plugin-css-injected-by-js": "^2.4.0",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.7",
    "@pinia/testing": "0.0.14",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "^29.2.3",
    "@types/jest-when": "^3.5.2",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.11.10",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.8.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^8.0.1",
    "jest-when": "^3.5.2",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.3",
    "vite": "^2.9.13",
    "vitest": "^0.9.3",
    "vue-tsc": "^1.0.10"
  }
}
